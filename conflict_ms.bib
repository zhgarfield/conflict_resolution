@article{knauft_violence_1991,
	title = {Violence and {Sociality} in {Human} {Evolution} [and {Comments} and {Replies}]},
	volume = {32},
	issn = {0011-3204},
	doi = {10.1086/203975},
	abstract = {Patterns of violence \& sociality found in simple human societies are compared to those documented among the four great-ape species \& the more complex prestate or middle-range human societies with regard to: food/resource sharing, dominance structure, male (M) competition for sexual access to females, \& violence \& intergroup competition. Results indicate that simple human societies exhibit a relative absence of competitive M dominance hierarchies \& of systemic violence between closed social groups, \& are more egalitarian among adult Ms politically, sexually, \& in resource sharing. A U-shaped evolutionary trajectory of selected features of human violence is proposed. It appears that simple human societies constitute a major anomaly for models suggesting evolutionary similarity between great-ape \& prestate human patterns of violence, without addressing the group-selection potential of symbolic transmission. It is urged that more consideration be given to cultural capacities in interaction with ecological constraints \& biogenetic selection in studying the evolution of human sociality \& violence. Comments are offered by: Thomas S. Abler (U of Waterloo, Ontario); Laura Betzig (U of Michigan, Ann Arbor); Christopher Boehm (Northern Kentucky U, Highland Heights); Robert Knox Dentan (State U of New York at Buffalo, Amherst); Thomas M. Kiefer (18 Worthington St, Boston, Mass); Keith F. Otterbein (State U of New York, Buffalo); John Paddock (Apt 464, 68000 Oaxaca, Mexico); \& Lars Rodseth (U of Michigan, Ann Arbor). In Reply, Knauft refutes specific criticisms, expands on several suggested viewpoints, offers additional insights on informal conflict mediation techniques, \& elaborates the purpose of the study. 457 References. Adapted from the source document.},
	language = {eng},
