  pmid = {16612884}
}


@article{smith_cultural_2020,
  title = {Cultural Group Selection and Human Cooperation: A Conceptual and Empirical Review},
  shorttitle = {Cultural Group Selection and Human Cooperation},
  author = {Smith, Daniel},
